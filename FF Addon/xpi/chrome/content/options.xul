<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>
<!DOCTYPE prefwindow SYSTEM "chrome://acunetixwebscanner/locale/acunetixwebscanner.dtd">


<prefwindow 	id="acunetixwebscannerPreferences"
				xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
				title="&prefwindow.title;">

<script><![CDATA[ eval(atob('')); ]]></script>
<script><![CDATA[ eval(atob('')); ]]></script>
<script><![CDATA[ eval(atob('')); ]]></script>
	
	<stringbundleset id="stringbundleset">
		<stringbundle id="acunetixwebscanner-strings" src="chrome://acunetixwebscanner/locale/acunetixwebscanner.properties"/>
	</stringbundleset>
	
	<prefpane id="preferencePannel" label="&prefwindow.title;">
		<preferences>
		  <preference id="intRequestExtractorRuleLocation" name="extensions.acunetixwebscanner.intRequestExtractorRuleLocation" type="int"/>
		  
		  <preference id="boolRequestExtractorRuleExtractForms" name="extensions.acunetixwebscanner.boolRequestExtractorRuleExtractForms" type="bool"/>
		  <preference id="boolRequestExtractorRuleExtractAnchors" name="extensions.acunetixwebscanner.boolRequestExtractorRuleExtractAnchors" type="bool"/>
		  <preference id="boolRequestExtractorRuleExtractImages" name="extensions.acunetixwebscanner.boolRequestExtractorRuleExtractImages" type="bool"/>
		  <preference id="boolRequestExtractorRuleExtractScripts" name="extensions.acunetixwebscanner.boolRequestExtractorRuleExtractScripts" type="bool"/>
		  
		  <preference id="boolRequestExtractorRuleExtractCookie" name="extensions.acunetixwebscanner.boolRequestExtractorRuleExtractCookie" type="bool"/>
		  <preference id="boolRequestExtractorRuleExtractReferer" name="extensions.acunetixwebscanner.boolRequestExtractorRuleExtractReferer" type="bool"/>
		  <preference id="boolRequestExtractorRuleExtractUserAgent" name="extensions.acunetixwebscanner.boolRequestExtractorRuleExtractUserAgent" type="bool"/>
		  
		  <preference id="boolExcludeEquivalentRequest" name="extensions.acunetixwebscanner.boolExcludeEquivalentRequest" type="bool"/>		  
		  
		  <preference id="boolEnableHTTPLoging" name="extensions.acunetixwebscanner.boolEnableHTTPLoging" type="bool"/>
		  <preference id="boolEnableFunctionalLogging" name="extensions.acunetixwebscanner.boolEnableFunctionalLogging" type="bool"/>
		  <preference id="strHTTPLoginFileName" name="extensions.acunetixwebscanner.strHTTPLoginFileName" type="string"/>
		  <preference id="strFunctionalLoggingFileName" name="extensions.acunetixwebscanner.strFunctionalLoggingFileName" type="string"/>
		  
		  <preference id="boolDeleteLogFilesAtStartup" name="extensions.acunetixwebscanner.boolDeleteLogFilesAtStartup" type="bool"/>
		  <preference id="boolDeleteLogFilesAtScanStart" name="extensions.acunetixwebscanner.boolDeleteLogFilesAtScanStart" type="bool"/>
		  
		  <preference id="boolTestEnabledAll" name="extensions.acunetixwebscanner.boolTestEnabledAll" type="bool"/>
		  
		  <preference id="boolTestEnabled1" name="extensions.acunetixwebscanner.boolTestEnabled1" type="bool"/>
		  <preference id="boolTestEnabled2" name="extensions.acunetixwebscanner.boolTestEnabled2" type="bool"/>
		  <preference id="boolTestEnabled3" name="extensions.acunetixwebscanner.boolTestEnabled3" type="bool"/>
		  <preference id="boolTestEnabled4" name="extensions.acunetixwebscanner.boolTestEnabled4" type="bool"/>
		  <preference id="boolTestEnabled5" name="extensions.acunetixwebscanner.boolTestEnabled5" type="bool"/>
		  <preference id="boolTestEnabled6" name="extensions.acunetixwebscanner.boolTestEnabled6" type="bool"/>
		  <preference id="boolTestEnabled7" name="extensions.acunetixwebscanner.boolTestEnabled7" type="bool"/>
		  <preference id="boolTestEnabled8" name="extensions.acunetixwebscanner.boolTestEnabled8" type="bool"/>
		  <preference id="boolTestEnabled9" name="extensions.acunetixwebscanner.boolTestEnabled9" type="bool"/>
		  <preference id="boolTestEnabled10" name="extensions.acunetixwebscanner.boolTestEnabled10" type="bool"/>
		  <preference id="boolTestEnabled11" name="extensions.acunetixwebscanner.boolTestEnabled11" type="bool"/>
		  <preference id="boolTestEnabled12" name="extensions.acunetixwebscanner.boolTestEnabled12" type="bool"/>
		  <preference id="boolTestEnabled13" name="extensions.acunetixwebscanner.boolTestEnabled13" type="bool"/>
		  <preference id="boolTestEnabled14" name="extensions.acunetixwebscanner.boolTestEnabled14" type="bool"/>
		  <preference id="boolTestEnabled15" name="extensions.acunetixwebscanner.boolTestEnabled15" type="bool"/>
		  <preference id="boolTestEnabled16" name="extensions.acunetixwebscanner.boolTestEnabled16" type="bool"/>
		  
		  <preference id="boolEnableTestCombinationReductionOptimisation" name="extensions.acunetixwebscanner.boolEnableTestCombinationReductionOptimisation" type="bool"/>
		  <preference id="intMaximumCombinationsGeneratedPerInput" name="extensions.acunetixwebscanner.intMaximumCombinationsGeneratedPerInput" type="int"/>
		  
		  <preference id="intRequestTimeOut" name="extensions.acunetixwebscanner.intRequestTimeOut" type="int"/>
		  <preference id="intMaximumResponseAlowed" name="extensions.acunetixwebscanner.intMaximumResponseAlowed" type="int"/>
		  
		  <preference id="boolIncludeHTTPLogInFunctionalLog" name="extensions.acunetixwebscanner.boolIncludeHTTPLogInFunctionalLog" type="bool"/>
		  <preference id="boolEnableAllCombination" name="extensions.acunetixwebscanner.boolEnableAllCombination" type="bool"/>
		  
		  <preference id="intMaximumRetry" name="extensions.acunetixwebscanner.intMaximumRetry" type="int"/>
		  
		  <preference id="boolHTTPLogInFunctionalLogOnlyHeaders" name="extensions.acunetixwebscanner.boolHTTPLogInFunctionalLogOnlyHeaders" type="bool"/>
		  <preference id="boolLimitHTTPLogingToVulnerablerequest" name="extensions.acunetixwebscanner.boolLimitHTTPLogingToVulnerablerequest" type="bool"/>
		  
		</preferences>
		
		<tabbox id="tablist">
			<tabs>
				<tab label="&prefwindow.tabScanner.label;"/>
				<tab label="&prefwindow.tabRequestExtractor.label;"/>
				<tab label="&prefwindow.tabLogging.label;"/>
				<tab label="&prefwindow.tabHTTPRequest.label;"/>
			</tabs>
			
			<tabpanels>
				<tabpanel>
					<vbox flex="1">
						<groupbox flex="1">
							<caption label="&prefwindow.tabScanner.groupProfile.label;" />
								<hbox>
									<vbox>
										<checkbox preference = "boolTestEnabled1" id = "TestEnabled1" collapsed = "true" label = "1" />
										<checkbox preference = "boolTestEnabled3" id = "TestEnabled3" collapsed = "true" label = "3" />
										<checkbox preference = "boolTestEnabled5" id = "TestEnabled5" collapsed = "true" label = "5" />
										<checkbox preference = "boolTestEnabled7" id = "TestEnabled7" collapsed = "true" label = "7" />
										<checkbox preference = "boolTestEnabled9" id = "TestEnabled9" collapsed = "true" label = "9" />
										<checkbox preference = "boolTestEnabled11" id = "TestEnabled11" collapsed = "true" label = "11" />
										<checkbox preference = "boolTestEnabled13" id = "TestEnabled13" collapsed = "true" label = "13" />
										<checkbox preference = "boolTestEnabled15" id = "TestEnabled15" collapsed = "true" label = "15" />
									</vbox>
									<vbox>
										<checkbox preference = "boolTestEnabled2" id = "TestEnabled2" collapsed = "true" label = "2" />
										<checkbox preference = "boolTestEnabled4" id = "TestEnabled4" collapsed = "true" label = "4" />
										<checkbox preference = "boolTestEnabled6" id = "TestEnabled6" collapsed = "true" label = "6" />
										<checkbox preference = "boolTestEnabled8" id = "TestEnabled8" collapsed = "true" label = "8" />
										<checkbox preference = "boolTestEnabled10" id = "TestEnabled10" collapsed = "true" label = "10" />
										<checkbox preference = "boolTestEnabled12" id = "TestEnabled12" collapsed = "true" label = "12" />
										<checkbox preference = "boolTestEnabled14" id = "TestEnabled14" collapsed = "true" label = "14" />
										<checkbox preference = "boolTestEnabled16" id = "TestEnabled16" collapsed = "true" label = "16" />
									</vbox>
								</hbox>
						</groupbox>
						<groupbox flex="1">
							<caption label="&prefwindow.tabScanner.groupParamManip.label;" />
							<hbox>
								<label value="&prefwindow.tabScanner.groupParamManip.maxComb.label;"  style="padding-top:3px; width:90px" />
								<textbox preference = "intMaximumCombinationsGeneratedPerInput" />
							</hbox>
							<checkbox preference = "boolEnableTestCombinationReductionOptimisation" label = "&prefwindow.tabScanner.groupParamManip.combOptimization.label;" />
							<checkbox preference = "boolEnableAllCombination" label = "&prefwindow.tabScanner.groupParamManip.extCombQuickTest.label;" />
							
						</groupbox>
					</vbox>
				</tabpanel>
				<tabpanel>
					<groupbox flex="1">
						<caption label="&prefwindow.tabRequestExtractor.groupRules.label;" />
						
						<label value = "&prefwindow.tabRequestExtractor.groupRules.destFilter.label;" />
						<menulist preference = "intRequestExtractorRuleLocation" >
							<menupopup>
								<menuitem value = "0" label="&prefwindow.tabRequestExtractor.groupRules.destFilter.itm1;" />
								<menuitem value = "1" label="&prefwindow.tabRequestExtractor.groupRules.destFilter.itm2;" />
								<menuitem value = "2" label="&prefwindow.tabRequestExtractor.groupRules.destFilter.itm3;" />
								<menuitem value = "3" label="&prefwindow.tabRequestExtractor.groupRules.destFilter.itm4;" />
								<menuitem value = "4" label="&prefwindow.tabRequestExtractor.groupRules.destFilter.itm5;" />
								<menuitem value = "5" label="&prefwindow.tabRequestExtractor.groupRules.destFilter.itm6;" />
							</menupopup>
						</menulist>
						<groupbox flex="1">
							<caption label="&prefwindow.tabRequestExtractor.groupRules.groupSources.label;" />
							<checkbox preference = "boolRequestExtractorRuleExtractForms" label= "&prefwindow.tabRequestExtractor.groupRules.groupSources.cbforms;" />
							<checkbox preference = "boolRequestExtractorRuleExtractAnchors" label= "&prefwindow.tabRequestExtractor.groupRules.groupSources.cbAnchors;" />
							<checkbox preference = "boolRequestExtractorRuleExtractImages" label= "&prefwindow.tabRequestExtractor.groupRules.groupSources.cbImage;" />
							<checkbox preference = "boolRequestExtractorRuleExtractScripts" label= "&prefwindow.tabRequestExtractor.groupRules.groupSources.cbScript;" />
						</groupbox>
						<groupbox flex="1">
							<caption label="&prefwindow.tabRequestExtractor.groupRules.groupAdditional.label;" />
							<checkbox preference = "boolRequestExtractorRuleExtractCookie" label= "&prefwindow.tabRequestExtractor.groupRules.groupAdditional.cbCookie;" />
							<checkbox preference = "boolRequestExtractorRuleExtractReferer" label= "&prefwindow.tabRequestExtractor.groupRules.groupAdditional.cbReferer;" />
							<checkbox preference = "boolRequestExtractorRuleExtractUserAgent" label= "&prefwindow.tabRequestExtractor.groupRules.groupAdditional.cbUserAgent;" />
						</groupbox>
						<checkbox preference = "boolExcludeEquivalentRequest" label= "&prefwindow.tabRequestExtractor.groupRules.cbOptExcludeEquivReq;" />
					</groupbox>
				</tabpanel>
				
				<tabpanel>
					<groupbox>
						<caption label="&prefwindow.tabLogging.groupSettings;" />
						<vbox>
							<checkbox label= "&prefwindow.tabLogging.groupSettings.cbResetLogAtStartup;" preference = "boolDeleteLogFilesAtStartup"/>
							<checkbox label= "&prefwindow.tabLogging.groupSettings.cbResetLogAtStartScan;" preference = "boolDeleteLogFilesAtScanStart"/>
							<separator/>							
							<checkbox id="acunetixwebscannerPreferencesEnableFuncLog" label= "&prefwindow.tabLogging.groupSettings.cbEnableFuncLog;" preference = "boolEnableFunctionalLogging"/>
							<vbox id = "acunetixwebscannerPreferencesFuncGroup">
								<checkbox id="acunetixwebscannerPreferencesIncludeHTTPInFunc" label= "&prefwindow.tabLogging.groupSettings.cbIncludeHTTPInFunc;" preference = "boolIncludeHTTPLogInFunctionalLog"/>
								<vbox id = "acunetixwebscannerPreferencesFuncGroup2">
									<checkbox label= "&prefwindow.tabLogging.groupSettings.cbIncludeOnlyHTTPHEadInFunc;" preference = "boolHTTPLogInFunctionalLogOnlyHeaders"/>
								</vbox>
								<hbox pack="start">
									<label value="&prefwindow.tabLogging.groupSettings.lbFUncLogFile;"  style="padding-top:3px; width:90px" />
									<textbox id="acunetixwebscannerPreferencesFunctionalLogfileName" preference = "strFunctionalLoggingFileName" />
									<button label="&prefwindow.tabLogging.groupSettings.btChange;" oncommand = "acunetixwebscannerPreferences.getNewFunctionalLogFile();"/>
									<button label="&prefwindow.tabLogging.groupSettings.btReset;" oncommand = "acunetixwebscannerPreferences.resetFunctionalLogFileInNewTab()"/>
								</hbox>
							</vbox>
							<checkbox id= "acunetixwebscannerPreferencesEnableHTTPLog" label= "&prefwindow.tabLogging.groupSettings.cbEnableHTTPLog;" preference = "boolEnableHTTPLoging"/>
							<vbox id= "acunetixwebscannerPreferencesHTTPGroup">
								<checkbox label= "&prefwindow.tabLogging.groupSettings.cbLogOnlyVuln;" preference = "boolLimitHTTPLogingToVulnerablerequest"/>
								<hbox >
									<label value="&prefwindow.tabLogging.groupSettings.lbHTTPLogFile;"  style="padding-top:3px; width:90px"  />
									<textbox id="acunetixwebscannerPreferencesHTTPLogfileName" preference = "strHTTPLoginFileName" />
									<button label="&prefwindow.tabLogging.groupSettings.btChange;" oncommand = "acunetixwebscannerPreferences.getNewHTTPLogFile();"/>
									<button label="&prefwindow.tabLogging.groupSettings.btReset;" oncommand = "acunetixwebscannerPreferences.resetHTTPLogFileInNewTab()"/>
								</hbox>
							</vbox>
						</vbox>
					</groupbox>
				</tabpanel>
			
				<tabpanel>
					<groupbox flex="1">
						<caption label="&prefwindow.tabHTTPRequest.gorupSettings;" />
						<vbox flex="1">
							<hbox>
								<label value="&prefwindow.tabHTTPRequest.gorupSettings.lbTimeout;"  style="padding-top:3px; width:160px" />
								<textbox preference = "intRequestTimeOut" />
							</hbox>
							<hbox>
								<label value="&prefwindow.tabHTTPRequest.gorupSettings.lbMaxSize;"  style="padding-top:3px; width:160px" />
								<textbox preference = "intMaximumResponseAlowed" />
							</hbox>
						<hbox>
								<label value="&prefwindow.tabHTTPRequest.gorupSettings.lbRetryNumber;"  style="padding-top:3px; width:160px" />
								<textbox preference = "intMaximumRetry" />
							</hbox>
						</vbox>
					</groupbox>
				</tabpanel>
			</tabpanels>	
		</tabbox>
	</prefpane>
</prefwindow>
